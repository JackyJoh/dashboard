{
  "rewrites": [
    { 
      "source": "/api/(.*)", 
      "destination": "https://nchdashboard.onrender.com/api/$1" 
    },
    { 
      "source": "/(.*)", 
      "destination": "/index.html" 
    } 
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}